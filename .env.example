# TinaCMS 配置
# 访问 https://app.tina.io 注册并获取你的 clientId
NEXT_PUBLIC_TINA_CLIENT_ID=your_tina_client_id_here
TINA_TOKEN=your_tina_token_here

# Snipcart 配置
# 访问 https://snipcart.com 注册并获取你的 API 密钥
NEXT_PUBLIC_SNIPCART_API_KEY=your_snipcart_api_key_here

# Google Analytics 配置
NEXT_PUBLIC_GA_TRACKING_ID=your_ga_tracking_id_here

# 其他环境变量
NODE_ENV=development
NEXT_PUBLIC_API_URL=http://localhost:3000/api